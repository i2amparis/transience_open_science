  </rdf:Description>
  <rdf:Description rdf:about="http://transience.eu/ontologies/Total_Scrap_Iron_and_Steel_Other">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://transience.eu/ontologies/Variable"/>
    <rdfs:label>Total Scrap|Iron and Steel|Other</rdfs:label>
